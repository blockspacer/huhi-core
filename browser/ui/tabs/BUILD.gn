source_set("tabs") {
  # Set due to //chrome/browser and //chrome/browser/ui circular dependencies.
  check_includes = false

  if (!is_android) {
    sources = [
      "huhi_tab_menu_model.cc",
      "huhi_tab_menu_model.h",
      "huhi_tab_strip_model.cc",
      "huhi_tab_strip_model.h",
    ]

    deps = [
      "//huhi/common/",
      "//chrome/app:generated_resources",
      "//components/prefs",
      "//components/sessions",
      "//content/public/browser",
    ]
  }
}
