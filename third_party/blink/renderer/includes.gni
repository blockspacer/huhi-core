  "//huhi/third_party/blink/renderer/*"
]

huhi_blink_renderer_core_public_deps = [
  "//huhi/third_party/blink/renderer"
]

# common includes which can help minimize patches for
# src/third_party/blink/renderer/modules/BUILD.gn
